<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_bg1"
        style="@style/image_view_style_dialog"
        android:src="@mipmap/reward_dialog_bg1" />

    <ImageView
        android:id="@+id/iv_bg2"
        style="@style/image_view_style_dialog"
        android:src="@mipmap/receiver_award_bg2" />

    <ImageView
        android:id="@+id/iv_bg3"
        style="@style/image_view_style_dialog"
        android:src="@mipmap/reward_dialog_bg3" />

    <FrameLayout
        style="@style/image_view_style_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv_bg4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingBottom="100dp"
            android:src="@mipmap/reward_dialog_bg4" />

        <ImageView
            android:id="@+id/iv_bg5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/reward_dialog_bg5" />

        <ImageView
            android:id="@+id/iv_bg6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/reward_dialog_bg6" />

        <ImageView
            android:id="@+id/iv_bg_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:src="@mipmap/reward_dialog_bg_left" />

        <ImageView
            android:id="@+id/iv_bg_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="25dp"
            android:layout_marginTop="20dp"
            android:src="@mipmap/reward_dialog_bg_right" />

    </FrameLayout>

    <ImageView
        android:id="@+id/iv_bg9"
        style="@style/image_view_style_dialog"
        android:layout_marginTop="25dp"
        android:src="@mipmap/reward_dialog_bg9" />

    <ImageView
        android:id="@+id/iv_star1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="100dp"
        android:src="@mipmap/reward_dialog_bg8" />

    <ImageView
        android:id="@+id/iv_star2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_gravity="center"
        android:layout_marginBottom="90dp"
        android:layout_marginRight="50dp"
        android:src="@mipmap/reward_dialog_bg8" />

    <ImageView
        android:id="@+id/iv_star3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="60dp"
        android:layout_marginRight="80dp"
        android:src="@mipmap/reward_dialog_bg8" />

    <ImageView
        android:id="@+id/iv_star4"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="110dp"
        android:src="@mipmap/reward_dialog_bg8" />

    <ImageView
        android:id="@+id/iv_star5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="110dp"
        android:src="@mipmap/reward_dialog_bg8" />

    <ImageView
        android:id="@+id/iv_star6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_gravity="center"
        android:layout_marginBottom="70dp"
        android:layout_marginLeft="80dp"
        android:src="@mipmap/reward_dialog_bg8" />
</FrameLayout>